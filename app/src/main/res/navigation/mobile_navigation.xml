<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.kpl.fragment.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_survey"
        android:name="com.kpl.fragment.SurveyFragment"
        android:label="@string/title_survey"
        tools:layout="@layout/fragment_servey" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.kpl.fragment.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_setting" />
    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.kpl.fragment.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>